<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>1.3.4</Version>
    <Authors>FranciscoNabas</Authors>
    <Platforms>x64</Platforms>
    <AssemblyName></AssemblyName>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <BaseIntermediateOutputPath />
    <LangVersion>10</LangVersion>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <DocumentationFile>bin\WindowsUtils\WindowsUtils.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Core\**" />
    <Compile Remove="UtilitiesLibrary\**" />
    <Compile Remove="Wrapper\**" />
    <EmbeddedResource Remove="Core\**" />
    <EmbeddedResource Remove="UtilitiesLibrary\**" />
    <EmbeddedResource Remove="Wrapper\**" />
    <None Remove="Core\**" />
    <None Remove="UtilitiesLibrary\**" />
    <None Remove="Wrapper\**" />
  </ItemGroup>

  <ItemGroup>
    <None Update="WindowsUtils.psd1">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="WindowsUtils.psm1">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="WindowsUtils.Types.ps1xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="PowerShellStandard.Library" Version="5.1.1" PrivateAssets="all" />
    <PackageReference Include="XmlDoc2CmdletDoc" Version="0.3.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="UtilitiesLibrary">
      <HintPath>StaticDependencies\UtilitiesLibrary.dll</HintPath>
    </Reference>
    <Reference Include="Wrapper">
      <HintPath>StaticDependencies\Wrapper.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
