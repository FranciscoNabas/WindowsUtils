<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>1.1.0</Version>
    <Authors>FranciscoNabas</Authors>
    <Platforms>x64</Platforms>
    <AssemblyName></AssemblyName>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <BaseIntermediateOutputPath />
    <LangVersion>10</LangVersion>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="UtilitiesLibrary\**" />
    <EmbeddedResource Remove="UtilitiesLibrary\**" />
    <None Remove="UtilitiesLibrary\**" />
  </ItemGroup>

  <ItemGroup>
    <None Update="WindowsUtils.dll-Help.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="WindowsUtils.psd1">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="PowerShellStandard.Library" Version="5.1.0-preview-06" PrivateAssets="all" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="UtilitiesLibrary">
      <HintPath>StaticDependencies\UtilitiesLibrary.dll</HintPath>
    </Reference>
    <Reference Include="Wrapper">
      <HintPath>StaticDependencies\Wrapper.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
